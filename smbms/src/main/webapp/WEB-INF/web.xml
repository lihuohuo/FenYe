<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>IUserServlet</servlet-name>
        <servlet-class>cn.happy.servlet.IUserServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>IUserServlet</servlet-name>
        <url-pattern>/IUserServlet</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>

</web-app>
